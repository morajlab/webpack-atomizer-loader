    },
    {
      "path": "dist/webpack-atomizer-plugin.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "husky": "^5.1.3",
    "size-limit": "^4.9.2",
